{
  "name": "modus-framework-styleguide",
  "version": "0.9.1",
  "homepage": "https://modus.trimble.com",
  "description": "The Modus Framework was developed as a common open source platform for all of Trimble's web applications",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.prod.js --colors",
    "build:dev": "webpack --config webpack/webpack.config.dev.js --colors",
    "deploy": "cross-env s3-deploy './build/**/*.{css,js,map,ico,png,jpg,svg,html}' --cwd './build/' --region us-east-1 --bucket prod-styles-trimblemaps --cache 0",
    "deploy:dev": "cross-env s3-deploy './build/**/*.{css,js,map,ico,png,jpg,svg,html}' --cwd './build/' --region us-east-1 --bucket dev-styles-trimblemaps --cache 0",
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "start:prod": "webpack-dev-server --open --config webpack/webpack.config.prod.js",
    "changelog": "shx mkdir -p .changelog && npm run changelog:fetch && npm run changelog:convert",
    "changelog:fetch": "git archive --remote=ssh://git@bitbucket.trimble.tools/tmds/modus-framework.git HEAD CHANGELOG.md -o ./.changelog/CHANGELOG.tar",
    "changelog:convert": "decompress --plugin=tar --out-dir=./.changelog ./.changelog/CHANGELOG.tar && showdown makehtml -i ./.changelog/CHANGELOG.md -o ./src/change-content.html",
    "svgo:base": "svgo --config=_svgo.yml --recursive",
    "svgo": "npm-run-all --parallel \"svgo:base -- --folder=src\""
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.trimble.tools/tmds/modus-styleguide.git"
  },
  "author": "Trimble",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@trimbleinc/modus-framework": "^0.9.1",
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "curlrequest": "^1.0.1",
    "decompress-cli": "^2.0.0",
    "decompress-tar": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "lit-loader": "^0.1.1",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "s3-deploy": "^1.4.0",
    "sass-loader": "^7.3.1",
    "showdown": "^1.9.1",
    "shx": "^0.3.2",
    "style-loader": "^0.23.1",
    "stylelint": "13.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^19.0.0",
    "svgo": "^1.3.2",
    "terser-webpack-plugin": "^1.4.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.0",
    "lit-element": "^2.2.1",
    "pretty": "^2.0.0",
    "prismjs": "^1.19.0"
  }
}
