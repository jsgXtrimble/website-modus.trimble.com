{
  "name": "ether-framework-styleguide",
  "version": "1.0.0",
  "description": "The Ether Framework was developed as a common open source platform for all of Trimble's web applications",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.prod.js --colors",
    "build:dev": "webpack --config webpack/webpack.config.dev.js --colors",
    "deploy": "cross-env s3-deploy './build/**/*.{css,js,map,ico,png,jpg,svg,html}' --cwd './build/' --region us-east-1 --bucket prod-styles-trimblemaps --cache 0",
    "deploy:dev": "cross-env s3-deploy './build/**/*.{css,js,map,ico,png,jpg,svg,html}' --cwd './build/' --region us-east-1 --bucket dev-styles-trimblemaps --cache 0",
    "start": "npm run changelog && webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "start:prod": "npm run changelog && webpack-dev-server --open --config webpack/webpack.config.prod.js",
    "changelog": "shx mkdir -p temp && git-file.bat",
    "changelog:convert": "decompress --plugin=tar --out-dir=./temp ./temp/CHANGELOG.tar && showdown makehtml -i ./temp/CHANGELOG.md -o ./src/change-content.html && shx rm -rf ./temp"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.trimble.tools/maps/project-ether-styleguide.git"
  },
  "author": "Trimble",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@trimblemaps/ether-framework": "^0.3.1",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "curlrequest": "^1.0.1",
    "decompress-cli": "^2.0.0",
    "decompress-tar": "^4.1.1",
    "eslint": "^5.12.1",
    "eslint-loader": "^2.1.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "lit-loader": "^0.1.0",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.11.0",
    "s3-deploy": "^1.3.0",
    "sass-loader": "^7.1.0",
    "showdown": "^1.9.0",
    "shx": "^0.3.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "lit-element": "^2.2.1",
    "pretty": "^2.0.0",
    "prismjs": "^1.15.0"
  }
}
